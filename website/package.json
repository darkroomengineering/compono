    "@studio-freight/tempus": "^0.0.38",
    "@theatre/core": "^0.7.0",
    "@theatre/studio": "^0.7.0",
    "clsx": "^2.0.0",
    "gsap": "3.12.2",
    "idb-keyval": "^6.2.1",
    "jsonminify": "^0.4.2",
    "next": "13.5.3",
    "next-sitemap": "^4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.68.0",
    "stats.js": "^0.17.0",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.1",
